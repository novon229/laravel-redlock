{
    "name": "novon229/laravel-redlock",
    "description": "Redis distributed locks for laravel - tweaked for laravel 10",
    "license": "MIT",
    "keywords": ["redlock", "laravel redis lock", "redis lock"],
    "authors": [
      {
        "name": "LibiChai",
        "email": "chaiguoxing@qq.com"
      },
      {
        "name": "Daniel Kuck-Alvarez",
        "email": "dankuck@gmail.com"
      },
      {
        "name": "Potsky",
        "email": "potsky@me.com"
      },
      {
          "name": "Brandon",
          "email": "novon229@gmail.com
    ],
    "require": {
      "php": ">=8.0.0",
      "illuminate/support": "^9.0",
      "illuminate/console": "^9.0",
      "predis/predis": "^2.0"
    },
    "require-dev": {
      "orchestra/testbench": "^7.0",
      "php-mock/php-mock-mockery": "^1.1",
      "phpunit/phpunit": "^9.0"
    },
    "autoload": {
      "psr-4": {
        "Novon229\\RedLock\\": "src/"
      }
    },
    "autoload-dev": {
      "classmap": [
        "tests/"
      ]
    },
    "extra": {
      "laravel": {
        "providers": [
          "Novon229\\RedLock\\RedLockServiceProvider"
        ]
      }
    },
    "scripts": {
      "test": [
        "phpunit"
      ]
    },
    "minimum-stability": "stable"
  }
  
